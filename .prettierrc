{
    "tabWidth": 4,
    "useTabs": false,
    "endOfLine": "auto",
    "trailingComma": "none",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "proseWrap": "never",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "arrowParens": "avoid",
    "bracketSpacing": true
}
